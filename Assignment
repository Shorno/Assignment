//Assignment1

#include <stdio.h>

int main() {
  printf("I\n");
  printf(" Want\n");
  printf("    to be a \n");
  printf("        Programmer\n");
  return 0;
}

//Assignment2

#include <stdio.h>

int main() {
  int a ,b,sum;
  printf("Input first number: ");
  scanf("%d", &a);
  printf("Input second number: ");
  scanf("%d", &b);

  sum=a+b;
  printf("Sum of %d and %d is %d", a,b, sum);
  return 0;
}

//Assignment3

#include <stdio.h>

int main() {
  int a ,b,temp;

  printf("Input first number: ");
  scanf("%d",&a);
  printf("Input first number: ");
  scanf("%d",&b);

  temp=b;
  b=a;
  a=temp;

  printf("Updated first number: %d\n",a);
  printf("Updated second number: %d\n",b);

  return 0;
}




//Assignment4

#include <stdio.h>

int main() {
  int a,b,c,d,e,total,avg;

  printf("Enter marks of five subjects: \n");

  scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);

  total= a+b+c+d+e;
  avg=total/5;

  printf("Total = %d\n",total);
  printf("Average = %d\n",avg);


  return 0;
}


//Assignment5

#include <stdio.h>

int main() {

    int x,i;
    printf("Enter the number: ");
    scanf("%d",&x);
    for( int i=1; i<=10; i++)

        printf("%dx%d = %d\n", x, i, x*i );

  return 0;
}





////Assignment6


#include <stdio.h>

int main() {

    int x,y,m,d;
    printf("Input number of days: ");
    scanf("%d",&x);
    y=x/365;
    m=(x%365)/30;
    d=(x-(y*365))-(m*30);

    printf("%d Year(s)\n", y);
    printf("%d Month(s)\n", m);
    printf("%d Day(s)", d);


  return 0;
}



